{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Ä°NNO Lead Reporter",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/server/index.ts",
      "env": {
        "NODE_ENV": "development",
        "STORAGE_TYPE": "memory"
      },
      "runtimeExecutable": "npx",
      "runtimeArgs": ["tsx"],
      "console": "integratedTerminal",
      "restart": true,
      "protocol": "inspector",
      "sourceMaps": true,
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    }
  ]
}